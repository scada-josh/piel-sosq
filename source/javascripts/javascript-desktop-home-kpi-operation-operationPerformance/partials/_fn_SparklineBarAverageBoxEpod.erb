SparklineBarAverageBoxEpod: function (arrDataSet) {
	// console.log("SparklineBarAverageBoxEpod");

	if (!jQuery().sparkline) {
		return;
	}
	$("#sparkline_bar_averageBox_Epod").sparkline(arrDataSet, {
		type: 'bar',
		width: '100',
		barWidth: 5,
		height: '55',
		barColor: '#f36a5b',
		negBarColor: '#e02222'
	});


  
},