                // Total over all pages
                if (api.column(9).data().length){
                    total = api
                        .column(9)
                        .data()
                        .reduce( function (a, b) {
                            return intVal(a) + intVal(b);
                        }, 0 );
                } else {
                    total = 0
                }

                // total = total/api.column(9).data().length;
                total = numeral(total).format('0,0');

                 // Total over this page

                 if (api.column(9).data().length){
                    var pageTotal = api
                    .column( 9, { page: 'current'} )
                    .data()
                    .reduce( function (a, b) {
                        return intVal(a) + intVal(b);
                    },0 ) 
                } else { 
